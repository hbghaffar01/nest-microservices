{
  "name": "starter-12-factor-app",
  "version": "2.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start": "ts-node ./src/main.ts",
    "start:debug": "nodemon",
    "tsc": "tsc",
    "build:prod": "npm run build",
    "start:prod": "node dist/src/main.js",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts",
    "db:drop": "npm run typeorm:cli schema:drop",
    "db:migration:generate": "typeorm migration:create -n",
    "db:create": "ts-node src/scripts/create-database.ts",
    "db:revert": "npm run typeorm:cli migration:revert",
    "db:sync": "npm run typeorm:cli schema:sync",
    "db:migration:run": "typeorm migration:run -t each"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.3",
    "@nestjs/core": "^10.4.3",
    "@nestjs/platform-express": "^10.4.3",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.2",
    "@types/sinon": "^17.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.11.0",
    "express": "^4.21.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "npm-check-updates": "^17.1.2",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sinon": "^19.0.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "url-join-ts": "^1.0.5",
    "winston": "^3.14.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.3",
    "@types/chai-spies": "^1.0.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "chai": "^5.1.1",
    "chai-spies": "^1.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslint": "6.1.3",
    "tslint-sonarts": "^1.9.0",
    "typescript": "5.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
